algoritmo "Aula 09 c"

var
contador, fim_contador: Inteiro

inicio
      contador <- 0
      Escreva("Até onde devo contar? ")
      Leia(fim_contador)
      
      Enquanto (contador <= fim_contador) faca
               Escreval(contador)
               contador <- contador + 1
      FimEnquanto
fimalgoritmo
