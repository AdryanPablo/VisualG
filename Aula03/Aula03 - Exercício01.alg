algoritmo "Aula 03 - Exercício 01"
var
   num1, num2: Inteiro
   soma: Real

inicio
      Escreva("Digite um número inteiro: ")
      Leia(num1)
      Escreva("Digite outro: ")
      Leia(num2)

      soma <- num1 + num2

      Escreval("A soma entre", num1, " e", num2, " é", soma, ".")
fimalgoritmo

